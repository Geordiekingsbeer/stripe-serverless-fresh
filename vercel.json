{
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://book.dineselect.co"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,DELETE,POST,PUT,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "X-CSRF-Token, X-Requested-With, Accept, Accept-Version, Content-Length, Content-MD5, Content-Type, Date, X-Api-Version"
        }
      ]
    }
  ],
  "rewrites": [
    { "source": "/admin-booking", "destination": "/api/admin-create-booking" },
    
    // --- FIX: Reroute traffic from old file name to new file name ---
    { "source": "/pick-seat.html", "destination": "/select-table.html" },
    
    // --- NEW: Add routing for the renamed file and old files ---
    { "source": "/select-table.html", "destination": "/select-table.html" },
    { "source": "/demo.html", "destination": "/demo.html" },
    
    // NOTE: If you deleted reports.html, you must remove this line.
    { "source": "/reports.html", "destination": "/reports.html" }, 
    
    { "source": "/admin.html", "destination": "/admin.html" },
    { "source": "/success.html", "destination": "/success.html" }
  ]
}
