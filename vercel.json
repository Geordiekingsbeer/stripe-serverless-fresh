{
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://geordiekingsbeer.github.io"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET,DELETE,POST,PUT,OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/webhook",
      "destination": "/api/webhook"
    }
  ]
}
